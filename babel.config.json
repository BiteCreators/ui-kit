{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "css-modules-transform",
      {
        "extensions": [".css"],
        "generateScopedName": "[name]__[local]___[hash:base64:5]",
        "keepImport": true
      }
    ]
  ]
}
